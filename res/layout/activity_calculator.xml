<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dip"
    android:padding="0dip"
    tools:context=".CalculatorActivity" >
    
    <Button
        android:id="@+id/button2"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button5"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_toRightOf="@+id/button1"
        android:text="2" />
    <!--
        style="?android:attr/buttonStyleSmall"
        -->

    <Button
        android:id="@+id/button5"
      
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button8"
        android:layout_alignLeft="@+id/button2"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="5" />
    <Button
        android:id="@+id/button8"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button0"
        android:layout_alignLeft="@+id/button2"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="8" />
   <Button
        android:id="@+id/buttonEQ"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/button3"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="=" />
    
    <Button
        android:id="@+id/button0"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_alignLeft="@+id/button2"
        android:text="0" />

    <Button
        android:id="@+id/button7"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button4"
        android:layout_above="@+id/button0"
   android:layout_margin="0dip"
    android:padding="0dip"
        android:text="7" />

    <Button
        android:id="@+id/button4"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button7"
        android:layout_alignLeft="@+id/button1"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="4" />

    <Button
        android:id="@+id/button1"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button4"
        android:layout_alignParentLeft="true"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="1" />
    
     <Button
        android:id="@+id/button3"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button6"
       android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_toRightOf="@+id/button2"
        android:text="3" />

    <Button
        android:id="@+id/button6"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button9"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_alignLeft="@+id/button3"
        android:text="5" />
    <Button
        android:id="@+id/button9"
       
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonEQ"
       android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_alignLeft="@+id/button3"
        android:text="9" />
    
    <Button
        android:id="@+id/buttonADD"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonSUB"
       android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_toRightOf="@+id/button3"
        android:text="+" />

    <Button
        android:id="@+id/buttonSUB"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonMUL"
        android:layout_alignLeft="@+id/buttonADD"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="-" />
    <Button
        android:id="@+id/buttonMUL"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonDIV"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_alignLeft="@+id/buttonADD"
        android:text="*" />
    
    <Button
        android:id="@+id/buttonDIV"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonADD"
        android:layout_margin="0dip"
    android:padding="0dip"

        android:layout_alignParentBottom="true"
        android:text="/" />
    
    <Button
        android:id="@+id/buttonLPar"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:layout_margin="0dip"
    android:padding="0dip"

        android:layout_above="@+id/button1"
        android:text="(" />
    <Button
        android:id="@+id/buttonRPar"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button2"
        android:layout_margin="0dip"
    android:padding="0dip"

        android:layout_above="@+id/button2"
        android:text=")" />
    <Button
        android:id="@+id/buttonDEL"
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonSIN"
        android:layout_margin="0dip"
    android:padding="0dip"
           android:layout_alignParentRight="true"
        android:layout_above="@+id/button2"
        android:text="DEL" />
    
    
    
    <WebView
        android:id="@+id/mathjaxview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/buttonLPar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" />
    
    <Button
        android:id="@+id/buttonLOG"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/buttonADD"
        android:layout_margin="0dip"
    android:padding="0dip"

        android:layout_alignParentBottom="true"
        android:text="log" />
    <Button
        android:id="@+id/buttonTAN"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonLOG"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:layout_alignLeft="@+id/buttonLOG"
        android:text="tan" />
     <Button
        android:id="@+id/buttonCOS"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonTAN"
        android:layout_alignLeft="@+id/buttonLOG"
        android:layout_margin="0dip"
    android:padding="0dip"
        android:text="cos" />
      <Button
        android:id="@+id/buttonSIN"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonCOS"
       android:layout_margin="0dip"
        android:padding="0dip"
        android:layout_alignLeft="@+id/buttonLOG"
        android:text="sin" />
<!--
   

   
    
    
     -->
    
    

</RelativeLayout>
